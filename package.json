{
  "name": "tdp-sound",
  "version": "0.1.0",
  "description": "Wordpress plugin to add and manage sounds",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "concurrently \"wp-scripts start\" \"gulp\" --names \"wp-scripts,      gulp\" --prefix name",
    "build": "yarn lint && rm -rf build && wp-scripts build && gulp build",
    "gulp": "gulp",
    "lint": "eslint --ignore-pattern '/build/*' --ignore-pattern '**/vendor/*' -o ./eslint_result.html -f html ."
  },
  "author": "@chrisservius",
  "private": true,
  "license": "GPLv3",
  "repository": "git@github.com:spidergon/tdp-sound.git",
  "homepage": "https://github.com/spidergon/tdp-sound#readme",
  "bugs": {
    "url": "https://github.com/spidergon/tdp-sound/issues"
  },
  "devDependencies": {
    "@babel/core": "^7.7.7",
    "@babel/preset-env": "^7.7.7",
    "@wordpress/scripts": "^6.1.0",
    "babel-eslint": "^10.0.3",
    "concurrently": "^5.0.2",
    "css-mqpacker": "^7.0.0",
    "cssnano": "^4.1.10",
    "del": "^5.1.0",
    "eslint": "^6.8.0",
    "eslint-config-alloy": "^3.5.0",
    "eslint-plugin-babel": "^5.3.0",
    "eslint-plugin-import": "^2.19.1",
    "eslint-plugin-node": "^11.0.0",
    "eslint-plugin-promise": "^4.2.1",
    "gulp": "^4.0.2",
    "gulp-cache": "^1.1.3",
    "gulp-if": "^3.0.0",
    "gulp-imagemin": "^6.2.0",
    "gulp-notify": "^3.2.0",
    "gulp-plumber": "^1.2.1",
    "gulp-postcss": "^8.0.0",
    "gulp-sass": "^4.0.2",
    "gulp-sourcemaps": "^2.6.5",
    "gulp-zip": "^5.0.1",
    "postcss-assets": "^5.0.0",
    "prettier": "^1.19.1",
    "vinyl-paths": "^3.0.1"
  },
  "prettier": {
    "endOfLine": "lf",
    "printWidth": 100,
    "semi": false,
    "singleQuote": true,
    "trailingComma": "none"
  }
}
